{
  "name": "tata-mes-bi",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:staging": "cross-env NODE_ENV=staging vite build --mode staging",
    "build:prod": "cross-env NODE_ENV=production vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type:check": "tsc --pretty --noEmit",
    "cspell:check": "cspell --no-progress --show-suggestions --show-context --cache **",
    "eslint:check": "eslint . --color --cache",
    "eslint:fix": "eslint . --color --cache --fix",
    "prettier:check": "prettier --check --cache --ignore-unknown .",
    "prettier:fix": "prettier --write --cache --ignore-unknown .",
    "cz": "git-cz",
    "prepare": "husky"
  },
  "dependencies": {
    "@bit-ocean/echarts": "^0.0.50",
    "@bit-ocean/hooks": "^0.0.50",
    "@bit-ocean/utils": "^0.0.50",
    "@tanstack/react-query": "^5.67.2",
    "@tanstack/react-router": "^1.112.18",
    "abortcontroller-polyfill": "^1.7.8",
    "axios": "^1.8.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "framer-motion": "^12.4.10",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resize-observer-polyfill": "^1.5.1",
    "use-immer": "^0.11.0",
    "vconsole": "^3.15.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@bit-ocean/auto-import": "^0.0.50",
    "@bit-ocean/cli": "^0.0.50",
    "@bit-ocean/commitlint-config": "^0.0.50",
    "@bit-ocean/cspell": "^0.0.50",
    "@bit-ocean/eslint-config": "^0.0.50",
    "@bit-ocean/prettier-config": "^0.0.50",
    "@bit-ocean/tsconfig": "^0.0.50",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@iconify/json": "^2.2.314",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tanstack/react-query-devtools": "^5.67.2",
    "@tanstack/router-devtools": "^1.112.18",
    "@tanstack/router-plugin": "^1.113.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.10",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-legacy": "^6.0.2",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cspell": "^8.17.5",
    "cz-git": "^1.11.1",
    "eslint": "8.57.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "sass": "^1.85.1",
    "tailwindcss": "3.4.17",
